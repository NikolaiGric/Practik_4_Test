<Window x:Class="Practik_4_Test.Users"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practik_4_Test"
        mc:Ignorable="d"
        Title="Users" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="3*"/>
        </Grid.RowDefinitions>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="3*"/>
                <ColumnDefinition Width="4*"/>
                <ColumnDefinition Width="4*"/>
            </Grid.ColumnDefinitions>
            <Button x:Name="Exit" Grid.Column="0" Content="Выйти" Click="Exit_Click"/>
            <Button x:Name="Editor" Grid.Column="1" Content="Редактировать" Click="Editor_Click"/>
            <Button x:Name="Using" Grid.Column="2" Content="Пройти тест" Click="Using_Click"/>
        </Grid>
            <Frame x:Name="AllFrame" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
